VITE_INJECT_APP_LOADING=true

# 打包后是否生成dist.zip
VITE_ARCHIVER=true
VITE_GLOB_OIDC_CALLBACK_URL=http://localhost:8090/front/auth/callback
  
VITE_GLOB_OIDC_AUTHORITY= 'http://localhost:9097/idx'
